<UserControl x:Class="CaseInstaller.Views.TrueManagementSetting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CaseInstaller.Views"
             mc:Ignorable="d" 
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid >
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Update" Margin="5" Command="{Binding GoForwardCommand}"/>
            <Button Content="Modify" Margin="5"  Command="{Binding ModifyCommand}" CommandParameter="TrueManagementOptions"/>
            <Button Content="Uninstall" Command="{Binding UninstallCommand}" CommandParameter="TrueManagementFinal" Margin="5" />
        </StackPanel>
        
    </Grid>
</UserControl>
